        <!-- Navigation -->
       
        <%- include('partials/nav.ejs') %>
        <!-- -----------------Main----------------- -->

        <!-- Title -->
        <img class = "rocky" src="images/rocky2.png">

        <div id="speechcal" class="speechbubblecontainercal">
            <div class="speechbubblecal">
                <div class="cal-circle"> 
                    <p class="calories"></p>
                    <p class="red-text"> kcal</p>
                </div> 
            </div>
        <div class="speechbubblearrowcal"></div>
        </div>
        
        
        <div>

            <h2 id = "title">I Ate a...</h2>

        </div>
    
        <!-- Search -->
        <form id="search-bar">
            <fieldset>
                <input type="text" id="search" placeholder="Rock">
            </fieldset>
        </form>
    
        <p id="para"></p>


        <!-- MODULES -->
        <!-- edible check and image input -->
        <div class="contentAfterSearch">
            <div class="devider"></div>

            <div class="container">

                <!-- image -->
                <img  class="itemImage"  src="images/image-placeholder.png">
                <p class="ContainerHeader" id="foodTitle"><b>Rock</b></p>
                
            </div>

            <div class="container">
                <div class="inner-container">
                    
                    <p class="ContainerHeaderExtras"></p>
                    <div class = "Macros" id="macros" >
                        <p class="servingSize"></p>
                        <p class="protein"></p>
                        <p class="saturatedFat"></p>
                        <p class="totalFat"></p>
                        <p class="fiber"></p>
                        <p class="sodium"></p>
                    </div>
                </div>
                




                <!-- N - fix list arrow -->


                <div class="inner-container" id="ContainerIngredient">
                    <p class="ContainerHeaderExtras" id="ingredientTitle"><b>Ingredients</b></p>
                    
                    <div class="ingredientsList" id="ingredientList"></div>
                </div>


                <!-- ------------------------------------------------------- -->






                


                <div class="centered-container">

                    <p><b>Want to add this to your tracker?</b></p>
                    <button id="show-tracker" class="trackerButton">Add</button>

                </div>

                <div id ="track" class="popup">

                    <div class="close-btn">&times;</div>
                        <div class="trackerPopup">
                            <h2>Track</h2>
                            <b class="trackerSubheader">Grams: </b>
                            <input id="gramsInput" type="number" placeholder="100" onKeyUp="displayMacros()" onChange="displayMacros()">
                            <b class ="macroName" id="cTB">Calories:</b>
                            <b class ="macroValue" id="caloriesM"></b><br>
                            <b class ="macroName" id="pTB">Protein:</b>
                            <b class ="macroValue" id="proteinM"></b><br>
                            <b class ="macroName" id="fTB">Fat:</b>
                            <b class ="macroValue" id="fatM"></b><br>
                            <a><button>Track!</button></a>
                        </div>

                    </div>
                </div>





                


                
                
            </div>


            <!-- N - working on back to top button -->


            <div class="devider"></div>



            <div class="container">

                <div class="centered-container">

                    <p><b>Not what you expected?</b></p>
                    <a href="meals"><button class="mealButton">Make My Own Meal</button></a>





                    
                </div>
            

            </div>


            

            <!-- ------------------------------------------------------- -->




            <!-- REMOVE LATER - BREAKS WITH SEARCH.JS -->

            <div class="devider"><br><br><br></div>
            <div class="container">

            </div>
            <!-- idk what this is, -->
            <div class = "Tracker" id="trackers" >
            </div>
        </div>



