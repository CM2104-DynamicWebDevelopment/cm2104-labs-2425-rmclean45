    <div class="loginpage">
        <%- include('partials/nav.ejs') %>
        <div class="containerContactUs">
            <h1>Contact Us!</h1>
            <form class="contactusform">
                <label for="email"><b>Email address</b></label><br>
                <input type="text" id="email" placeholder="YourEmail@gmail.com"><br><br>
                <label for="inputBox"><b>What's Up?</b></label><br>
                <textarea id="inputBox" placeholder="Description..."rows="4" cols="50"></textarea><br><br>
                <button id ="createButton" type="button">Send Message</button>
            </form>
        </div>

        <div id ="create" class="verifyPopup">
            <div class="close-btn">&times;</div>
            <div class="form">
                <h2>Message Sent</h2>
                <p>Your message has been sent to us. Lookout for an email within a few days!</p>
                <div class="form-element">
                    <button onclick="location.href='index.html'">OK!</button>
                </div><br><br>
            </div>
        </div>

        <!-- for testing scroll -->
        <br><br><br><br><br><br>

        <br><br><br><br><br><br>
        <br><br><br><br><br><br>

