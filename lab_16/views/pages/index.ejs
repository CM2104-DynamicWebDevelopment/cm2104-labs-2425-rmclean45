<html>

<head>
  <title>Socket.IO chat</title>
  <link rel="stylesheet" href="styles/main.css">
</head>

<body>
  <form id="userform">
    <input id="username" autocomplete="off" /><button id="senduser">Set Username</button>
  </form>
  <ul id="messages"></ul>
  <ul id="pmmessages"></ul>
  <form id="form" action="">
    <ul>
      <li><label>Send to user</label>
        <input id='sendto' autocomplete="off" />
        <label>Send to room</label>
        <input id='room' autocomplete="off" />
      </li>
      <li>
        <label>Message</label>
        <input id="input" autocomplete="off" /><button id="send">Send</button>
      </li>
    </ul>
  </form>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="scripts/main.js"></script>
</body>

</html>